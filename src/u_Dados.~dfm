  object q_Usuarios: TADOQuery
    Connection = Conexao
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT'
      '  u.IDUSUARIO,'
      '  u.NOMEUSUARIO,'
      '  u.NLOGIN,'
      '  u.SENHA,'
      '  u.EMAIL,'
      '  u.DTINCLUSAO,'
      '  u.DTALTERACAO,'
      '  u.USINCLUSAO,'
      '  u.USALTERACAO,'
      '  u.DEFINIRSENHA,'
      '  case u.SENHAOK when '#39'S'#39' then '#39'Sim'#39' else '#39'N'#227'o'#39' end SENHADEF,'
      
        '  (select b.nlogin from tbusuarios b where b.usinclusao=u.idusua' +
        'rio) lg_inclusao,'
